<resources>

    <style name="main_footer_linear">
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="main_footer_image">
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_width">22dp</item>
        <item name="android:layout_height">22dp</item>
        <item name="android:layout_marginBottom">1dp</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="main_footer_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:textColor">#999</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="ccc_1px">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">#ccc</item>
    </style>

    <!-- 自定义loading dialog -->
    <style name="selectorDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item><!--半透明-->
        <item name="android:windowNoTitle">true</item><!--无标题-->
        <item name="android:windowBackground">@drawable/selector_dialog_bg</item><!--背景透明-->
        <item name="android:backgroundDimEnabled">false</item><!--模糊-->
        <item name="android:backgroundDimAmount">0.6</item>
    </style>

    <!--toolbar 标题 -->
    <style name="page_title">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <!--toolbar -->
    <style name="app_toolbar">
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:layout_height">?actionBarSize</item>
        <item name="android:layout_width">match_parent</item>
    </style>
    <!--toolbar 返回键 -->
    <style name="back_img">
        <item name="android:layout_width">21dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/margin_left</item>
        <item name="android:paddingRight">3dp</item>
        <item name="android:src">@mipmap/back</item>
    </style>

    <!--无数据图片 -->
    <style name="no_data_img">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:src">@mipmap/wushuju</item>
        <item name="android:visibility">gone</item>
    </style>
    <!-- 资讯中查看更多 -->
    <style name="info_bottom">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginLeft">@dimen/margin_left</item>
        <item name="android:layout_marginRight">@dimen/margin_right</item>
        <item name="android:background">@drawable/info_bottom_bg</item>
    </style>

    <style name="info_bottom_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">15sp</item>
    </style>

    <!-- 登录注册界面 -->
    <style name="login_layout">
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginRight">40dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">44dp</item>
    </style>

    <style name="login_img">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_width">16dp</item>
        <item name="android:layout_height">16dp</item>
    </style>

    <style name="login_text">
        <item name="android:background">@null</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textColorHint">#fff</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="login_bottom">
        <item name="android:layout_marginRight">40dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="login_miss">
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_width">75dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textColor">#fff</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">11sp</item>
        <item name="android:layout_marginBottom">20dp</item>
        <item name="android:background">@drawable/miss_back</item>
    </style>

    <!-- 下一步 -->
    <style name="bottom_relative">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginLeft">@dimen/margin_left</item>
        <item name="android:layout_marginRight">@dimen/margin_right</item>
        <item name="android:background">@drawable/appointment_back</item>
    </style>

    <style name="bottom_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">15sp</item>
    </style>

    <!--新用户注册 获取验证码 -->
    <style name="register_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/margin_left</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#999</item>
    </style>

    <style name="register_text_right">
        <item name="android:background">@null</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">80dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColorHint">#bbb</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#222</item>
    </style>

    <style name="e2e2e2_1px">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">#e2e2e2</item>
    </style>
    <!--个人中心 -->
    <style name="mine_order_linear_fff">
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">#fff</item>
    </style>

    <style name="mine_order_linear">
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="mine_order_linear_img">
        <item name="android:layout_width">22dp</item>
        <item name="android:layout_height">22dp</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <style name="mine_order_linear_text">
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:textSize">11sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#222</item>
    </style>

    <style name="e2e2e2_high">
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">#e2e2e2</item>
    </style>

    <style name="animation">
        <item name="android:windowEnterAnimation">@anim/push_fade_in</item>
        <item name="android:windowExitAnimation">@anim/push_fade_out</item>
    </style>

    <style name="animation_bottom">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>

    <style name="userInfo_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/margin_left</item>
        <item name="android:textColor">#222</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="relative_fff_44">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:background">#fff</item>
    </style>

    <style name="setting_right_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">2dp</item>
        <item name="android:textColor">#666</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="userInfo_JT">
        <item name="android:layout_width">12dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">@dimen/margin_right</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:src">@mipmap/jiantou</item>
    </style>

    <style name="update_sex" parent="android:Theme.Light">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
</resources>
